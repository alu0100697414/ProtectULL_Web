<% include _head.ejs %>
<body ng-controller="HistorialCtrl">
<% include _navbar.ejs %>

<div class="container">
    <div class="row">
        <br><br>
        <h2 class="center indigo-text"> Historial de conexiones </h2>
    </div>

    <!-- Standard button -->
    <center><button ng-click="delete()" type="button" class="btn btn-default">ELIMINAR HISTORIAL</button></center>

    <br>

    <div class="row">
        <div class="col m12 s12">
            <table class="responsive-table bordered">
                <thead>
                <tr>
                    <th data-field="MAC">MAC</th>
                    <th data-field="nombre">Nombre</th>
                    <th data-field="numero">Número de teléfono</th>
                    <th data-field="Fecha">Fecha</th>
                </tr>
                </thead>
                <tbody>
                <tr ng-repeat="historial in historiales">
                    <td>{{historial.name}}</td>
                    <td>{{historial.nombre}}</td>
                    <td>{{historial.numero}}</td>
                    <td>{{historial.time}}</td>
                </tr>
                </tbody>
            </table>
        </div>
    </div>

</div>
<br><br>
<% include _footer.ejs %>
</body>
</html>
